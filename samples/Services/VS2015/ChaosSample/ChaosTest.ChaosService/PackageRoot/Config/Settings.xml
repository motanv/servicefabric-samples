  <Section Name="ChaosTestService">
    <Parameter Name="MaxClusterStabilizationTimeout" Value="" MustOverride="true"/>
    <Parameter Name="MaxConcurrentFaults" Value="" MustOverride="true"/>
    <Parameter Name="EnableMoveReplicaFaults" Value="" MustOverride="true"/>
    <Parameter Name="OperationTimeout" Value="" MustOverride="true"/>
    <Parameter Name="WaitTimeBetweenFaults" Value="" MustOverride="true"/>
    <Parameter Name="WaitTimeBetweenIterations" Value="" MustOverride="true"/>
